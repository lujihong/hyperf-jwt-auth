{
    "name": "lujihong/jwt-auth",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf"
    ],
    "authors": [
        {
            "name": "lujihong",
            "email": "382217590@qq.com",
            "role": "Developer"
        }
    ],
    "description": "jwt-auth Component",
    "autoload": {
        "psr-4": {
            "Hyperf\\JWTAuth\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HyperfTest\\": "tests"
        }
    },
    "require": {
        "php": ">=8.0",
        "lcobucci/jwt": "~4.1.0",
        "nesbot/carbon": "^1.0 || ^2.0"
    },
    "require-dev": {},
    "suggest": {
        "hyperf/utils": "required hyperf/utils ~3.0.0",
        "hyperf/cache": "required hyperf/cache ~3.0.0",
        "hyperf/command": "required hyperf/command ~3.0.0",
        "hyperf/config": "required hyperf/config ~3.0.0",
        "hyperf/di": "required hyperf/di ~3.0.0"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "scripts": {},
    "extra": {
        "hyperf": {
            "config": "Hyperf\\JWTAuth\\ConfigProvider"
        }
    }
}
